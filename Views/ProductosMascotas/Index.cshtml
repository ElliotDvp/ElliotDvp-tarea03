@model List<TuProyecto.Models.ProductoMascota>

<h1>Productos para Mascotas</h1>
<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Categor√≠a</th>
            <th>Precio</th>
            <th>Stock</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.Nombre</td>
                <td>@p.Categoria</td>
                <td>@p.Precio</td>
                <td>@p.Stock</td>
            </tr>
        }
    </tbody>
</table>
